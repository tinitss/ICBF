@page
@model ICBFApp.Pages.TipoDoc.IndexModel
@{
}
<br />
<h2>Lista de Tipo de documentos</h2>
<a class="btn btn-primary btn-sm" href="/TipoDoc/Create">Nuevo Tipo de Documento</a>

<table class="table">
    <thead>
        <tr>
            <th>Tipo</th>
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.listTipoDoc)
        {
            <tr>
                <td>@item.tipo</td>

                <td>
                    <a class="btn btn-primary btn-sm" href="/TipoDoc/Edit?id=@item.pkIdTipoDoc">Editar</a>
                    <a class="btn btn-danger btn-sm" href="/TipoDoc/Delete?id=@item.pkIdTipoDoc">Borrar</a>
                </td>
            </tr>
        }
    </tbody>
</table>
